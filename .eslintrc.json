{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "google",
    "prettier"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "max-len": "off",
    "require-jsdoc": "off"
  }
}